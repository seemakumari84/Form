            <input
              class="block px-5 py-2 w-1/4 outline-none rounded-md bg-zinc-800" name="previous" value="<%= filename  %>"
              placeholder="Previous name"
            />
            <input
            class="block px-5 py-2 w-1/4 outline-none rounded-md  mt-3 bg-zinc-800" name="new"
            placeholder="New Name"
          />
                    <input
              class="mt-3 px-4 py-1 rounded-md bg-yellow-600"
              type="submit"
              value="Update"
            />
          </form>
    </div>
